{% extends app.request.isXmlHttpRequest ? '@WebProfiler/Profiler/ajax_layout.html.twig' : '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="28" height="28" alt="Blimp" src="data:image/png;base64," />
        <span class="sf-toolbar-status{% if 50 < collector.callscount %} sf-toolbar-status-yellow{% endif %}">{{ collector.callscount }} Req.</span>

        {% if collector.callscount > 0 %}
            <span>in {{ '%0.0f'|format(collector.time) }} ms</span>
        {% endif %}
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>HTTP Calls</b>
            <span>{{ collector.callscount }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Calls time</b>
            <span>{{ '%0.0f'|format(collector.time) }} ms</span>
        </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon">
        <img width="28" height="28" alt="Blimp" src="data:image/png;base64," />
    </span>
    <strong>Guzzle</strong>
    <span class="count">
        <span>{{ collector.callscount }}</span>
        <span>{{ '%0.0f'|format(collector.time) }} ms</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    {{ block('calls') }}
{% endblock %}

{% block calls %}
    <h2>Calls</h2>

    {% if collector.calls is empty %}
        <p>
            <em>No calls.</em>
        </p>
    {% else %}
        <ul class="alt">
            {% for i, call in collector.calls %}
                <li class="{{ cycle(['odd', 'even'], i) }}">
<pre>
{{ call.request_method|raw }} {{ call.request_path|raw }}
{{ call.request_headers|raw }}

{{ call.response_headers|raw }}

{{ call.log|raw }}
</pre>
                    <br /><br />
                    <small>
                        <strong>Time</strong>: {{ '%0.2f'|format(call.executionMS) }} ms
                        <strong>Memory</strong>: {{ '%0.2f'|format(call.memory / 1024) }} KiB
                    </small>
                </li>
            {% endfor %}
        </ul>
    {% endif %}

{% endblock %}
